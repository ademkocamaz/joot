title 009-Windows Server 2003, Enterprise
map --unmap=0:0xff
map --unhook && map --rehook
# if %@root:~1,3%==hd0 && geometry (hd1) && map (hd0) (hd1) && map (hd1) (hd0) && map --rehook && set PART=%@root:~5,1% && root (hd1,%PART%) || echo !!!
set GZFILE=/joot/WINDOWS/SERVER/SERVER2003_BOOT.iso
set BOOTFILE=/I386/SETUPLDR.BIN
geometry (hd1) && checkrange 0x80 read 0x8280 && map (hd0) (hd1) && map (hd1) (hd0) && map --hook
if exist %GZFILE% || find --set-root --ignore-floppies %GZFILE% || echo -> Not found %GZFILE%
if exist %GZFILE% && set EXIST=1
if %EXIST%==1 && echo ----------------------------------------------------
if %EXIST%==1 && echo Loading Windows Server 2003, Enterprise
if %EXIST%==1 && echo Please wait...
if %EXIST%==1 && echo ----------------------------------------------------
if %EXIST%==1 && map --mem %GZFILE% (0xFF)
if %EXIST%==1 && map --e820cycles=3
if %EXIST%==1 && map --rehook && root (0xFF) && chainloader (0xFF)%BOOTFILE% || chainloader () && boot
boot || echo -> Fail to boot! && echo && pause --wait=5 <<< Return to clasic menu...
