# Grub4dos usb diskten çalýþtýrýldýðý için, birinci disk ile ikinci diskin yerini deðiþtiriyoruz!
title 001-Windows 10 6 in 1 x64 17 Ocak 2016
echo Windows 10 6 in 1 x64 17 Ocak 2016
set ISOFILE=/joot/Windows/10/TR_WINDOWS_10_6IN1_VERSION_1511_X64_BOOT.ISO
geometry (hd1) && checkrange 0x80 read 0x8280 && map (hd0) (hd1) && map (hd1) (hd0) && map --hook
# map (hd0) (hd1) && map (hd1) (hd0) && map --hook   # sorun olursa bunu kullan
find --set-root --ignore-floppies %ISOFILE%
echo file: && ls %ISOFILE% && echo
map %ISOFILE% (0xFF)
map --hook
root (0xFF)
chainloader (0xFF)/BOOTMGR || chainloader (0xFF) || chainloader ()
boot
